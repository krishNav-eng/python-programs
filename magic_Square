# "This is the program to crate a magic square!!")

import numpy as np
from random import randint

class latin_square:
  def grid(self):
    self.size = int(input("enter the dimensions for the magic square: "))
    self.grid = np.zeroes((self.size, self.size))
    
  def draft(self):
    for y in range(self.size):
      for x in range(self.size):
        a = randint(1, self.size + 1)
        self.grid[y][x] = a
        
  def improve(self):
    for y in range(self.size):
      enteries = []
      for x in range(self.size):
        entery = self.grid[y][x]
        enteries.append(entery)
      new = randint(1, self.size)
      
